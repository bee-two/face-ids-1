<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data Ip</title>
    <%- include("../admin/layout/head.ejs") %>
</head>
<body>
    <div class="container">

        <div class="panel panel-default">
            <div class="panel-heading">
                Kết quả trả về từ Title: <b><%= data.title %></b> 
            </div>

            <div class="panel-body">
                <div class="table-responsive">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <!-- <th>Title</th> -->
                                    <!-- <th>Link truy cập</th> -->
                                    <th>Ip</th>
                                    <th>Info</th>
                                    <th>Location</th>
                                    <th>Thời gian tạo</th>
                                    <th>Tương tác</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if (data && data.dataip){%>
                                    <% for (var i=0; i < data.dataip.length; i++) { %>
                                    <tr>
                                        <td><%= data.dataip[i].id %> </td> 
                                        <!-- <td> <a href="/data/<= data.dataip[i].title %>">data.dataip[i].title %></a> </td> -->
                                        <td><%= data.dataip[i].getip %> </td>
                                        <td><%= data.dataip[i].useragent %> </td> 
                                        <td><%- data.dataip[i].data_geol %> </td>
                                        <td><%= data.dataip[i].created_at %> </td>  
                                        <td>
                                            <!--  -->
                                            <button href="/data/delete" class="btn btn-danger post_delete" post_id="<%= data.dataip[i].id %>">Xóa</button>
                                        </td>
                                    </tr>
                                    <% } %>
                                <% } %>  

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div> 
    <script type="text/javascript" src="/static/js/dataip.js"></script>
</body>
</html>